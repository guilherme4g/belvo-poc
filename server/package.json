{
  "name": "node-leap",
  "version": "0.0.0",
  "description": "Node Leap is a node boilerplate that follows best practicles of software design and agile",
  "main": "dist/main/server.js",
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "build:local": "rimraf dist && npm run build",
    "start": "node dist/main/server.js",
    "start:local": "npm run prisma:migration:push && npx nodemon --exec 'npm run build:local && npm run start' --watch src -e ts",
    "start:watch": "npm run prisma:migration:push && npx nodemon --exec 'npm run start' --watch src -e ts",
    "lint": "eslint 'src/**' '__tests__/**'",
    "lint:fix": "eslint 'src/**' '__tests__/**' --fix",
    "style:check": "prettier --check 'src/**/*.ts' '__tests__/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "test": "jest --passWithNoTests --noStackTrace --runInBand --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js --verbose",
    "test:staged": "npm test -- --findRelatedTests --runInBand",
    "test:ci": "npm test -- --coverage",
    "prisma:generate": "npx prisma generate --schema=./src/main/infra/prisma/schema.prisma",
    "prisma:migration": "npx prisma migrate --schema=./src/main/infra/prisma/schema.prisma --name",
    "prisma:migration:push": "npx prisma migrate deploy --schema=./src/main/infra/prisma/schema.prisma",
    "plop": "plop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loomi/node-leap.git"
  },
  "author": "itd@cin.ufpe.br",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/loomi/node-leap/issues"
  },
  "homepage": "https://github.com/loomi/node-leap#readme",
  "devDependencies": {
    "@types/belvo": "^0.16.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.3",
    "@types/jest": "^27.5.0",
    "@types/pino": "^6.3.12",
    "@types/pino-http": "^5.8.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autocannon": "^7.9.0",
    "clinic": "^12.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "factory.ts": "^1.2.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "jest-ts-auto-mock": "^2.1.0",
    "lint-staged": "^11.2.3",
    "nodemon": "^2.0.14",
    "pino-pretty": "^7.2.0",
    "plop": "^2.7.6",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.3.0",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^27.1.4",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "@sentry/node": "^6.14.0",
    "@sentry/tracing": "^6.14.0",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.1178.0",
    "belvo": "^0.25.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "6.6.0",
    "helmet": "^4.6.0",
    "module-alias": "^2.2.2",
